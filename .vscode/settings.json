    "latex-workshop.latex.recipe.default": "arara",
    "spellright.ignoreRegExps": [
        "/\\\\thm@[a-z]+/g",
        "/\\\\@[a-z]+/g",
        "/\\\\let[a-z@\\\\]+/g"
    ],
    "spellright.ignoreRegExpsByClass": {
        "latex" : [ 
            "/\\\\@ifpackagelater{.+}/g",
            "/\\\\begin{minted}[^]*?\\\\end{minted}/gm",
            "/\\\\begin{cminted}[^]*?\\\\end{cminted}/gm",
            "/\\\\begin{tabular}[^]*?\\\\end{tabular}/gm"
        ]
    },
    "spellright.notificationClass": "warning"
